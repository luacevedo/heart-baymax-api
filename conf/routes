# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                                      controllers.Application.index

# Rules
GET     /rules                                 controllers.RulesController.getRules()

# Patient
GET     /patients/attributes                   controllers.PatientsController.getPatientAttributes()

# Diagnosis fields
GET     /patients/fields                       controllers.DiagnosisFieldsController.getPatientInputFields(preliminary: Option[Boolean])

